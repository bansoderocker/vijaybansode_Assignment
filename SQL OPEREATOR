
--1	LIST ALL EMPS WHO ARE WORKING IN DEPT 20 AND EARNING MORE THEN 2500
SELECT * FROM EMP WHERE DEPTNO = 20 AND SAL>2500

--2	LIST ALL EMPS WHOSE JOB IS EITHER MANAGER OR CLERK OR ANALYST
SELECT * FROM EMP WHERE JOB ='MANAGER' OR JOB = 'CLERK' OR JOB = 'ANALYST'

--3	LIST ALL EMPS WHO ARE WORKING AS CLERK IN DPET 10 OR MANAGER FOR DEPT 30
SELECT * FROM EMP WHERE (JOB = 'CLERK' and DEPTNO = 1 ) OR (JOB ='MANAGER' and DEPTNO = 30 )


--4	LIST ALL EMPS WHOSE SALARY IN RANGE OF 3000 TO 5000

SELECT * FROM EMP WHERE SAL >3000 AND SAL <5000


--5	LIST ALL EMPLOYEES WHOSE COMM IS MORE THEN SALARY
SELECT * FROM EMP WHERE COMM > SAL

--6 LIST ALL EMPS WHOSE ARE EARNING COMMISSION
SELECT * FROM EMP WHERE COMM >0



